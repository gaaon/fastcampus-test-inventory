          <stringProp name="parameters"></stringProp>
          <stringProp name="script">try {
	FileWriter fWriter = new FileWriter(&quot;utils/find-duplicate/result.txt&quot;, true);
	BufferedWriter buff = new BufferedWriter(fWriter);
	
	buff.write(vars.get(&quot;stock&quot;));
	buff.newLine();
	
	buff.close();
	fWriter.close();
} catch (Exception e) {
	throw new RuntimeException(e);
}</stringProp>
